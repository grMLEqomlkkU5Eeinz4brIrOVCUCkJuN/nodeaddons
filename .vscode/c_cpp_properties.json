{
	"configurations": [
		{
			"name": "Windows",
			"includePath": [
				"${workspaceFolder}/**",
				"${workspaceFolder}/node_modules/node-addon-api",
				"${userProfile}/.node-gyp/*/include/node"
			],
			"defines": [
				"NAPI_VERSION=6",
				"NODE_ADDON_API_DISABLE_DEPRECATED",
				"_WIN32"
			],
			"cStandard": "c17",
			"cppStandard": "c++17",
			"intelliSenseMode": "windows-msvc-x64"
		},
		{
			"name": "Linux",
			"includePath": [
				"${workspaceFolder}/**",
				"/usr/include/node"
			],
			"defines": [],
			"compilerPath": "/usr/bin/clang",
			"cStandard": "c17",
			"cppStandard": "c++17",
			"intelliSenseMode": "linux-clang-x64"
		}
	],
	"version": 4
}